rcnorm <- function(n,mean,sd1,sd2,prob){
x0 = rnorm(n, mean, sd=sd1)
x1 = rnorm(n, mean, sd=sd2)
flag = rbinom(n, size=1, prob)
return(x0*(1-flag) + x1*flag)
}
